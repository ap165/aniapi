{
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ],
    "functions": {
      "app/main.py": {
        "maxDuration": 30
      },
      "app/api.py": {
        "maxDuration": 30
      }
    }
  }
